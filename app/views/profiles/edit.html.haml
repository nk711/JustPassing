.container#wrapped
	#profile_edit.row
		.col-md-8.col-md-offset-2
			.panel.panel-default
				.panel-body
					-#Content body, renders an edit form
					%h1= t('.title')
					=render 'form'
				.panel-footer
					-#Footer
					.row
						.col-md-12
							.btn-group.pull-right
								= link_to t('back'), root_path, class: "btn btn-default"
